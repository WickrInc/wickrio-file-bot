{
  "name": "wickrio-file-bot",
  "version": "6.8.2",
  "description": "WickrIO File Bot",
  "main": "file_bot.js",
  "dependencies": {
    "dotenv": "^8.2.0",
    "prompt": "^1.0.0",
    "wickrio-bot-api": "6.8.x"
  },
  "scripts": {
    "start": "nohup wpm2 start ./wpm.json >>wpm2.output 2>&1 & echo $! > $(cat pidLocation.json)",
    "stop": "kill $(cat $(cat pidLocation.json))",
    "restart": "kill $(cat $(cat pidLocation.json)) && nohup wpm2 start ./wpm.json >>wpm2.output 2>&1 & echo $! > $(cat pidLocation.json)"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WickrInc/wickrio-file-bot"
  },
  "keywords": [
    "wickr",
    "wickrio",
    "wickrbot",
    "bot",
    "bots",
    "secure",
    "messaging",
    "api"
  ],
  "database": false
}
